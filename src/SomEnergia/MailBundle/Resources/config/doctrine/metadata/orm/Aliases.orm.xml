<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Aliases" table="aliases">
    <id name="pkid" type="smallint" column="pkid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mail" type="string" column="mail" length="120" nullable="false"/>
    <field name="destination" type="string" column="destination" length="120" nullable="false"/>
    <field name="enabled" type="boolean" column="enabled" nullable="false"/>
  </entity>
</doctrine-mapping>
