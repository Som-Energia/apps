<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Users" table="users">
    <id name="id" type="string" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="128" nullable="false"/>
    <field name="uid" type="smallint" column="uid" nullable="false"/>
    <field name="gid" type="smallint" column="gid" nullable="false"/>
    <field name="home" type="string" column="home" length="255" nullable="false"/>
    <field name="maildir" type="string" column="maildir" length="255" nullable="false"/>
    <field name="enabled" type="boolean" column="enabled" nullable="false"/>
    <field name="changePassword" type="boolean" column="change_password" nullable="false"/>
    <field name="clear" type="string" column="clear" length="128" nullable="false"/>
    <field name="crypt" type="string" column="crypt" length="128" nullable="false"/>
    <field name="quota" type="string" column="quota" length="255" nullable="false"/>
    <field name="procmailrc" type="string" column="procmailrc" length="128" nullable="false"/>
    <field name="spamassassinrc" type="string" column="spamassassinrc" length="128" nullable="false"/>
  </entity>
</doctrine-mapping>
